{
  "name": "@slub/musiconn-performance-data-pipeline",
  "description": "musiconn.performance data transformation pipelines and utilities",
  "version": "1.0.0",
  "scripts": {
    "test": "jest",
    "slurp": "sh ./scripts/slurp.sh",
    "build:graph": "cross-env NODE_OPTIONS=--max-old-space-size=12288 ts-node ./src/dumpToRDF.ts",
    "build:jsonSchema": "ts-node ./src/buildJsonSchema.ts",
    "build:rust": "ts-node ./src/buildRustTypes.ts",
    "build:types": "ts-node ./src/buildTypes.ts",
    "build:rdf": "cross-env NODE_OPTIONS=--max-old-space-size=12288 concurrently -m 1 --raw 'npm:build:rdf:event' 'npm:build:rdf:corporation' 'npm:build:rdf:person'  'npm:build:rdf:subject'  'npm:build:rdf:work' 'npm:build:rdf:series' 'npm:build:rdf:location'",
    "build:rdf:event": "ts-node ./src/dumpToRDF.ts -o ./data/rdf -t event -i ./data",
    "build:rdf:corporation": "ts-node ./src/dumpToRDF.ts -o ./data/rdf -t corporation -i ./data",
    "build:rdf:location": "ts-node ./src/dumpToRDF.ts -o ./data/rdf -t location -i ./data",
    "build:rdf:person": "ts-node ./src/dumpToRDF.ts -o ./data/rdf -t person -i ./data",
    "build:rdf:subject": "ts-node ./src/dumpToRDF.ts -o ./data/rdf -t subject -i ./data",
    "build:rdf:work": "ts-node ./src/dumpToRDF.ts -o ./data/rdf -t work -i ./data",
    "build:rdf:series": "ts-node ./src/dumpToRDF.ts -o ./data/rdf -t series -i ./data",
    "build:cypher": "cross-env NODE_OPTIONS=--max-old-space-size=12288 concurrently -m 1 --raw 'npm:build:cypher:event' 'npm:build:cypher:corporation' 'npm:build:cypher:person'  'npm:build:cypher:subject'  'npm:build:cypher:work' 'npm:build:cypher:series' 'npm:build:cypher:location'",
    "build:cypher:event": "ts-node ./src/rdfToCypher.ts -o ./data/cypher -t event -i ./data/rdf/event.ttl",
    "build:cypher:corporation": "ts-node ./src/rdfToCypher.ts -o ./data/cypher -t corporation -i ./data/rdf/corporation.ttl",
    "build:cypher:location": "ts-node ./src/rdfToCypher.ts -o ./data/cypher -t location -i ./data/rdf/location.ttl",
    "build:cypher:person": "ts-node ./src/rdfToCypher.ts -o ./data/cypher -t person -i ./data/rdf/person.ttl",
    "build:cypher:subject": "ts-node ./src/rdfToCypher.ts -o ./data/cypher -t subject -i ./data/rdf/subject.ttl",
    "build:cypher:work": "ts-node ./src/rdfToCypher.ts -o ./data/cypher -t work -i ./data/rdf/work.ttl",
    "build:cypher:series": "ts-node ./src/rdfToCypher.ts -o ./data/cypher -t series -i ./data/rdf/series.ttl",
    "pipeline:dump2neo4j": "ts-node ./src/fromDumpToNeo4J.ts",
    "doc:jsonschema": "mkdir -p docs/jsonschema && jsonschema2md -o docs/jsonschema -e json -d data/schema/",
    "fix:rdf": "sh ./scripts/fix_ttl_files.sh ./data/rdf ./data/fixed",
    "lint": "eslint . --ext .ts"
  },
  "author": "Sebastian Tilsch <sebastian-tilsch@slub-dresden.de>",
  "license": "MIT",
  "dependencies": {
    "@adobe/jsonschema2md": "^7.1.3",
    "@asgerf/strongcli": "^0.3.0",
    "@elastic/elasticsearch": "^8.4.0",
    "@rdfine/rdf": "^0.5.7",
    "@rdfjs/data-model": "1.3.4",
    "@rdfjs/dataset": "^1.1.1",
    "@rdfjs/types": "^1.1.0",
    "@tpluscode/rdf-ns-builders": "^2.0.1",
    "@tpluscode/rdf-string": "^0.2.26",
    "@tpluscode/rdfine": "^0.5.41",
    "ascii-progress": "^2.0.0",
    "clownface": "^1.5.1",
    "dayjs": "^1.11.6",
    "elasticdump": "^6.90.0",
    "express": "^4.18.2",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "n3": "^1.16.2",
    "ndjson": "^2.0.0",
    "neo4j-driver": "^4.4.0",
    "progress-stream": "^2.0.0",
    "stronger-typed-streams": "^0.2.0",
    "ws": "^8.10.0"
  },
  "devDependencies": {
    "@types/clownface": "^1.5.1",
    "@types/express": "^4.17.14",
    "@types/highland": "^2.12.14",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/n3": "^1.10.4",
    "@types/ndjson": "^2.0.1",
    "@types/node": "^18.11.2",
    "@types/progress-stream": "^2.0.2",
    "@types/rdf-ext": "^1.3.11",
    "@types/rdfjs__data-model": "^2.0.1",
    "@types/rdfjs__dataset": "^2.0.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.4",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "jest": "^29.2.2",
    "prettier": "2.7.1",
    "quicktype": "^15.1.0",
    "quicktype-core": "^6.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typeconv": "^1.8.0",
    "typescript": "^4.8.4"
  }
}
